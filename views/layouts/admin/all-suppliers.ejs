                            <%= item.landline %>
                          </td>
                          <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter font-bold">
                            <%= item.phone %>
                          </td>
                          <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold"><%= item.email %></td>
