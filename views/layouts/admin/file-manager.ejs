<script>
  function openNewFolder() {
    const popup = document.querySelector("#popup_new_folder")

    popup.classList.remove("hidden")
  }

  function closeNewFolder() {
    const popup = document.querySelector("#popup_new_folder")

    popup.classList.add("hidden")
  }

  function createNewFolder() {
    const name_folder = document.querySelector("#name_folder")

    if (name_folder.value.length == 0) {
      name_folder.style.border = "1px solid #e00d0d;"
      return
    }

    fetch('/admin/file-manager/create-folder', {
      method: 'POST',
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({name_folder: name_folder.value})
    })
    .then((response) => {
      if (response.ok) {
        location.reload()
      } else {
        alertMessage("error", "Erro ao criar pasta")
      }
    })
    .catch((error) => {
      alertMessage("error", "Tente novamente mais tarde")
    })
  }


  function openUploadArchive() {
    const popup = document.querySelector("#popup_upload_archive")

    popup.classList.remove("hidden")
  }

  function closeUploadArchive() {
    const popup = document.querySelector("#popup_upload_archive")

    popup.classList.add("hidden")
  }

  document.getElementById('uploadForm').addEventListener('submit', function(event) {
    event.preventDefault();
    const formData = new FormData(this);

    fetch('/admin/file-manager/upload-archive', {
      method: 'POST',
      body: formData,
    })
    .then(response => response.text())
    .then(message => {
      location.reload()
    })
    .catch(error => {
      console.error('Erro ao enviar arquivo:', error);
    });
  });
</script>
