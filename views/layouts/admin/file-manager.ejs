                    <input type="file" name="archive" id="archive" class="block w-full rounded-lg p-1.5 text-sm text-gray-900 border-blue-1 shadow-sm ring-1 ring-inset inter">
                  </div>
                </div>

                <div class="mt-4">
                  <label for="name" class="block text-md font-medium leading-6 text-gray-900 inter mt-2">Pasta:</label>
                  <div class="flex items-center option mt-2">
                    <select name="folders" id="folders" class="block w-full rounded-lg inter py-2 mt-2 pl-2 text-gray-900 ring-1 ring-inset ring-gray-300 sm:text-sm sm:leading-6 border-blue-1">
                      <option value="no">NÃ£o</option>
                      <% folders.forEach(folder => { %>
                        <option value="<%= folder %>" selected><%= folder %></option>
                      <% }) %>
                    </select>
                  </div>
                </div>
              </div>

              <div class="pb-4">
                <button type="submit" class="w-full py-2.5 inter bg-blue text-white text-sm rounded-lg mt-4">Fazer upload</button>
              </div>
            </form>
          </div>
        </div>
      </div>

