            </div>
          </div>
        </div>

        <div class="bg-white rounded-xl shadow-lg border col-span-3">
          <div class="p-2 flow-root">
            <div class="inline-block min-w-full align-middle">
              <% if (calls.length > 0) { %>
                <div class="overflow-hidden shadow ring-1 ring-black ring-opacity-5 sm:rounded-lg">
                  <table class="min-w-full divide-y divide-gray-300">
                    <thead class="bg-gray-50">
                      <tr>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Mensagem</th>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">SLA</th>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Início em</th>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Término em</th>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Status</th>
                        <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Ações</th>
                      </tr>
                    </thead>

                    <tbody class="divide-y divide-gray-200 bg-white">
                      <% calls.forEach(function(item) { %>
                        <tr>
                          <td class="whitespace-nowrap px-3 py-4 text-sm inter text-gray-800 font-bold">
                            <%= item.description %>
                          </td>
  
                          <% if (item.status == "pending" || item.status == "in-attendance" && !item.confirmed_code) { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">Não iniciado</td>
                          <% } else if (item.status == "in-attendance" && item.confirmed_code) { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-yellow-500 inter color-purple font-semibold"><%= item.sla %> (contando)</td>
                          <% } else if (item.status == "concluded") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold"><%= item.sla %></td>
                          <% } else if (item.status == "canceled") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-red-500 inter color-purple font-semibold">Cancelado</td>
                          <% } %>
  
                          <% if (item.status == "pending" || item.status == "in-attendance" && !item.confirmed_code) { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold">Aguardando</p>
                            </td>
                          <% } else if (item.status == "in-attendance" && item.confirmed_code) { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold"><%= item.start_time %></p>
                            </td>
                          <% } else if (item.status == "canceled") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold">Não iniciado</p>
                            </td>
                          <% } else { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold"><%= item.start_time %></p>
                            </td>
                          <% } %>
  
                          <% if (item.status == "pending" || item.status == "in-attendance" && !item.confirmed_code) { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold">Aguardando</p>
                            </td>
                          <% } else if (item.status == "in-attendance") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold">Em andamento</p>
                            </td>
                          <% } else if (item.status == "canceled") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold">Não concluído</p>
                            </td>
                          <% } else if (item.status == "concluded") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <p class="inter font-bold"><%= item.end_time %></p>
                            </td>
                          <% } %>
  
                          <% if (item.status == "pending") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                <div class="flex-none rounded-full p-1 text-yellow-400">
                                  <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                </div>
                                <div class="hidden text-yellow-400 sm:block">Pendente</div>
                              </div>
                            </td>
                          <% } else if (item.status == "in-attendance") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                <div class="flex-none rounded-full p-1 text-green-400">
                                  <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                </div>
                                <div class="hidden text-green-400 sm:block font-bold">Atendendo</div>
                              </div>
                            </td>
                          <% } else if (item.status == "concluded") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                <div class="flex-none rounded-full p-1 text-blue-500">
                                  <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                </div>
                                <div class="hidden text-blue-500 sm:block">Concluído</div>
                              </div>
                            </td>
                          <% } else if (item.status == "canceled") { %>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                <div class="flex-none rounded-full p-1 text-red-600 bg-red-opacity">
                                  <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                </div>
                                <div class="hidden text-red-600 sm:block">Cancelado</div>
                              </div>
                            </td>
                          <% } %>
  
                          <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                            <a href="/technician/view-call/<%= item._id %>" class="flex items-center">
                              <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" class="mr-2">
                                <path opacity="0.5" d="M2.73038 12.7463C2.0221 11.8262 1.66797 11.3661 1.66797 9.99992C1.66797 8.63375 2.0221 8.17369 2.73038 7.25353C4.1446 5.41622 6.51639 3.33325 10.0013 3.33325C13.4862 3.33325 15.858 5.41622 17.2722 7.25353C17.9805 8.17369 18.3346 8.63375 18.3346 9.99992C18.3346 11.3661 17.9805 11.8262 17.2722 12.7463C15.858 14.5836 13.4862 16.6666 10.0013 16.6666C6.51639 16.6666 4.1446 14.5836 2.73038 12.7463Z" stroke="#0E1726" stroke-width="1.25"/>
                                <path d="M12.5 10C12.5 11.3807 11.3807 12.5 10 12.5C8.61925 12.5 7.5 11.3807 7.5 10C7.5 8.61925 8.61925 7.5 10 7.5C11.3807 7.5 12.5 8.61925 12.5 10Z" stroke="#0E1726" stroke-width="1.25"/>
                              </svg>
                              Visualizar                                  
                            </a>
                          </td>
                        </tr>
                      <% }) %>
                    </tbody>
                  </table>
                </div>
              <% } else { %>
                <div class="flex flex-col justify-center items-center px-4 sm:px-2 mt-2 col-span-3 mt-32">
                  <svg width="280" height="140" viewBox="0 0 280 140" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <g clip-path="url(#clip0_5_5)">
                    <mask id="mask0_5_5" style="mask-type:luminance" maskUnits="userSpaceOnUse" x="0" y="0" width="280" height="140">
                    <path d="M279.277 0H0V140H279.277V0Z" fill="white"/>
                    </mask>
