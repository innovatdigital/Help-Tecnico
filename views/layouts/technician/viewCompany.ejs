      <div class="grid grid-cols-2 sm:grid-cols-2 lg:grid-cols-3 gap-x-6 mt-6">
        <% if (company.service == "pmoc") { %>
          <div class="bg-white rounded-xl shadow-lg border p-4 px-5 col-span-1">
            <div class="flex">
              <h2 class="inter font-bold text-gray-700 text-lg">PMOC da empresa:</h2>
            </div>

            <h1 class="inter font-semibold text-blue">Nenhum PMOC disponivel</h1>
            <iframe src="/pmoc.pdf" id="pre" class="w-full mt-3 rounded-lg" height="400" allowfullscreen webkitallowfullscreen></iframe>
          </div>
        <% } else if (company.service == "equipment-list") { %>
          <div class="bg-white rounded-xl shadow-lg border p-4 px-5 col-span-1">
            <div class="flex">
              <h2 class="inter font-bold text-gray-700 text-lg">Lista de equipamentos da empresa:</h2>
            </div>

            <h1 class="inter font-semibold text-blue">Nenhuma lista de equipamentos disponivel</h1>
            <iframe src="/equipments.pdf" class="w-full mt-3 rounded-lg" height="400" allowfullscreen webkitallowfullscreen></iframe>
          </div>
        <% } %>

        <div class="bg-white rounded-xl shadow-lg border p-4 px-5 col-span-1">
          <div class="flex">
            <h2 class="inter font-bold text-gray-700 text-lg">Equipamentos da empresa:</h2>
          </div>

          <h1 class="inter font-semibold text-blue">Nenhum equipamento disponivel</h1>

          <ul role="list" class="mt-4 space-y-2">
            <% equipments.forEach(item => { %>
              <li class="flex items-center space-x-4 rounded-lg border">
                <div>
                  <img class="w-32 h-20 rounded-l-lg" src="/img/equipments/<%= item.imageFile %>" alt="">
                </div>

                <div>
                    <h2 class="text-sm font-bold text-gray-800 inter"><%= item.model %></h2>
                    <h2 class="text-xs mt-0.5 font-semibold text-gray-800 inter">Marca: <span class="text-blue"><%= item.brand %></span></h2>
                    <div>
                      <a href="/technician/view-equipment/<%= item._id %>" class="text-blue mt-2 text-sm font-semibold inter underline">Ver equipamento</a>
                    </div>
                </div>
              </li>
            <% }) %>
          </ul>
        </div>
      </div>
    </main>
  </div>
