<script>
  let idCompany;

  function openFunction(id) {
    idCompany = id

    const companies = document.querySelector("#popup_companies")
    const register = document.querySelector("#popup_register")

    companies.classList.add("hidden")
    register.classList.remove("hidden")
  }

  async function saveEquipment() {
    const model = document.querySelector("#model")
    const brand = document.querySelector("#brand")

    const response = await fetch(`/technician/save-equipment/${idCompany}`, {
      method: "PUT",
      headers: { 'Content-Type': 'application/json' },
      body: JSON.stringify({ model: model.value, brand: brand.value })
    });

    if (response.ok) {
      const data = await response.json();

      location.href = `/technician/view-equipment?idCompany=${idCompany}&idEquipment=${data.id}`;
    } else {
      alert_message("error", "Não foi possível cadastrar o equipamento.");
    }
  }

  function openLoading(boolean) {
    const popup_await = document.querySelector("#popup_await")

    if (boolean) {
      popup_await.classList.remove("hidden")
    } else {
      popup_await.classList.add("hidden")
    }
  }
</script>

