                    <path d="M21.5182 16.646L21.4956 16.5177C21.3008 15.4106 21.0339 13.8945 20.0228 13.1397C19.5964 12.8214 19.1136 12.6637 18.6467 12.5111C18.4628 12.451 18.2893 12.3942 18.1239 12.3293C17.8828 12.2346 17.6656 12.1418 17.4621 12.0393C16.6073 13.18 15.4225 13.7806 14.0132 13.7807C12.6043 13.7807 11.4197 13.1801 10.5649 12.0393C10.3613 12.1419 10.1441 12.2346 9.90297 12.3294C9.7378 12.3942 9.56413 12.451 9.3803 12.5111C8.91331 12.6637 8.43045 12.8215 8.00413 13.1397C6.9927 13.8946 6.72601 15.4111 6.53124 16.5183L6.50869 16.6462C6.34595 17.5652 6.24882 18.4567 6.22009 19.2959C6.18637 20.2804 6.79747 20.4999 7.33657 20.6936L7.39463 20.7145C8.04293 20.9484 8.76814 21.1488 9.61162 21.327C11.334 21.6906 12.7318 21.8641 14.0101 21.8732H14.0134H14.0167C15.2953 21.8641 16.6929 21.6906 18.415 21.327C19.2587 21.1489 19.9838 20.9485 20.6319 20.7145L20.6899 20.6937C21.2291 20.5 21.8404 20.2806 21.8067 19.2958C21.778 18.4544 21.6808 17.563 21.5182 16.646Z" fill="white"/>
                    <path d="M10.1124 8.11814C10.3676 10.3467 11.3424 13.0597 14.0518 13.0596C16.7612 13.0595 17.7361 10.3467 17.9913 8.11814C18.3057 5.37294 17.0075 3.30688 14.0518 3.30688C11.0966 3.30679 9.79758 5.37272 10.1124 8.11814Z" fill="white"/>
                  </svg>                    
                </div>
                <p class="ml-16 truncate text-sm inter font-medium text-gray-500">Grupos importados</p>
              </dt>
              <dd class="ml-16 flex items-baseline pb-6 sm:pb-7">
                <p class="text-xl font-semibold color-purple"><%= total_groups %> grupos</p>
              </dd>
            </div>

            <div class="relative overflow-hidden rounded-lg bg-white px-4 pt-5 shadow sm:px-6 sm:pt-6">
              <dt>
                <div class="absolute rounded-lg bg-purple p-2.5">
                  <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M2.33203 5.83325C2.33203 3.90026 3.89904 2.33325 5.83203 2.33325H22.1654C24.0984 2.33325 25.6654 3.90026 25.6654 5.83325V22.1666C25.6654 24.0996 24.0984 25.6666 22.1654 25.6666H5.83203C3.89904 25.6666 2.33203 24.0996 2.33203 22.1666V5.83325ZM5.83203 4.66659C5.1877 4.66659 4.66536 5.18893 4.66536 5.83325V11.6666H23.332V5.83325C23.332 5.18893 22.8097 4.66659 22.1654 4.66659H5.83203ZM4.66536 13.9999V22.1666C4.66536 22.8109 5.1877 23.3333 5.83203 23.3333H22.1654C22.8097 23.3333 23.332 22.8109 23.332 22.1666V13.9999H4.66536ZM16.332 15.1666C16.6414 15.1666 16.9382 15.2896 17.157 15.5083L21.8236 20.175C22.2792 20.6306 22.2792 21.3693 21.8236 21.8249C21.3681 22.2805 20.6293 22.2805 20.1737 21.8249L16.332 17.9832L13.657 20.6582L12.4903 21.8249C12.0347 22.2805 11.296 22.2805 10.8404 21.8249C10.3848 21.3693 10.3848 20.6306 10.8404 20.175L11.1821 19.8333L10.4987 19.1498L7.82366 21.8249C7.36804 22.2805 6.62935 22.2805 6.17374 21.8249C5.71813 21.3693 5.71813 20.6306 6.17374 20.175L9.67374 16.675C9.89253 16.4562 10.1893 16.3333 10.4987 16.3333C10.8081 16.3333 11.1049 16.4562 11.3237 16.675L12.832 18.1834L15.5071 15.5083C15.7258 15.2896 16.0226 15.1666 16.332 15.1666ZM12.832 8.16659C12.832 7.52226 13.3543 6.99992 13.9987 6.99992H19.832C20.4764 6.99992 20.9987 7.52226 20.9987 8.16659C20.9987 8.81091 20.4764 9.33325 19.832 9.33325H13.9987C13.3543 9.33325 12.832 8.81091 12.832 8.16659ZM8.16536 10.2083C9.29295 10.2083 10.207 9.29417 10.207 8.16659C10.207 7.039 9.29295 6.12492 8.16536 6.12492C7.03778 6.12492 6.1237 7.039 6.1237 8.16659C6.1237 9.29417 7.03778 10.2083 8.16536 10.2083Z" fill="white"/>
                  </svg>                    
                </div>
                <p class="ml-16 truncate text-sm inter font-medium text-gray-500">Publicações realizadas</p>
              </dt>
              <dd class="ml-16 flex items-baseline pb-6 sm:pb-7">
                <p class="text-xl font-semibold color-purple"><%= total_posts %> publicações</p>
                <span class="ml-2 flex items-center gap-x-1.5 rounded-full badge px-2 py-0.5 text-xs font-medium color-purple mt-0.5">
                  <svg class="h-1.5 w-1.5 fill-green-500" viewBox="0 0 6 6" aria-hidden="true">
                    <circle cx="3" cy="3" r="3" />
                  </svg>
                  <% if (type_account == "Basico") { %>
                    <%= 100 - total_posts %> restantes
                  <% } else if (type_account == "Pro") { %>
                    <%= 300 - total_posts %> restantes
                  <% } else if (type_account == "Avançado") { %>
                    Ilimitado
                  <% } %>
                </span>
              </dd>
            </div>

            <div class="relative overflow-hidden rounded-lg bg-white px-4 pt-5 shadow sm:px-6 sm:pt-6">
              <dt>
                <div class="absolute rounded-lg bg-purple p-2.5">
                  <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M15 2.5C8.1 2.5 2.5 8.1 2.5 15C2.5 21.9 8.1 27.5 15 27.5C21.9 27.5 27.5 21.9 27.5 15C27.5 8.1 21.9 2.5 15 2.5ZM15 6.25C17.075 6.25 18.75 7.925 18.75 10C18.75 12.075 17.075 13.75 15 13.75C12.925 13.75 11.25 12.075 11.25 10C11.25 7.925 12.925 6.25 15 6.25ZM15 24C11.875 24 9.1125 22.4 7.5 19.975C7.5375 17.4875 12.5 16.125 15 16.125C17.4875 16.125 22.4625 17.4875 22.5 19.975C20.8875 22.4 18.125 24 15 24Z" fill="white"/>
                  </svg>                    
                </div>
                <p class="ml-16 truncate text-sm inter font-medium text-gray-500">Plano atual</p>
              </dt>
              <dd class="ml-16 flex items-baseline pb-6 sm:pb-7">
                <p class="text-xl font-semibold color-purple"><%= type_account %></p>
                <a href="/#plans" class="ml-2 flex items-center gap-x-1.5 rounded-full badge px-2 py-0.5 text-xs font-medium color-purple mt-0.5">
                  <svg class="h-1.5 w-1.5 fill-green-500" viewBox="0 0 6 6" aria-hidden="true">
                    <circle cx="3" cy="3" r="3" />
                  </svg>
                  <% if (type_account == "Basico") { %>
                    Faça upgrade
                  <% } else if (type_account == "Pro") { %>
                    Faça upgrade
                  <% } else if (type_account == "Avançado") { %>
                    Melhor plano
                  <% } %>
                </a>
              </dd>
            </div>
          </dl>
        </div>
      </div>

      <div class="grid grid-cols-1 sm:grid-cols-1 lg:grid-cols-4 lg:pr-12">
        <div class="px-4 sm:px-6 lg:pl-12 mt-8 col-span-3">
          <div class="sm:flex sm:items-center">
            <div class="sm:flex-auto">
              <h1 class="text-lg font-semibold leading-6 text-gray-900 inter">Publicações recentes:</h1>
            </div>
          </div>
          
          <div class="mt-4 flow-root">
            <div class="lg:-mx-4 lg:-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
              <div class="inline-block min-w-full py-2 align-middle sm:px-6 lg:px-8">
                <% if (posts.length > 0) { %>
                  <div class="overflow-hidden shadow ring-1 ring-black ring-opacity-5 rounded-lg">
                    <table class="min-w-full divide-y divide-gray-300">
                      <thead class="bg-gray-50">
                        <tr>
                          <th scope="col" class="py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 inter">Imagem</th>
                          <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Descrição</th>
                          <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Conta</th>
                          <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Grupos e páginas</th>
                          <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Plataforma</th>
                          <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-gray-900 inter">Status</th>
                        </tr>
                      </thead>

                      <tbody class="divide-y divide-gray-200 bg-white">
                        <% posts.forEach(function(item) { %>
                          <tr>
                            <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-6">
                              <% if (item.image == "program_image") { %>
                                <img class="h-10" src="/img/admin/program.png" alt="img">
                              <% } else if (item.image == "link") { %>
                                <img class="h-10" src="/img/platform/link-externo.png" alt="img">
                              <% } else if (item.image == "post") { %>
                                <img class="h-9" src="/img/admin/post.png" alt="img">
                              <% } else { %>
                                <img class="h-10 rounded-md" src="<%= item.image %>" alt="img">
                              <% } %>
                            </td>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter">
                              <p style="width: 140px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;" class="inter"><%= item.content %></p>
                            </td>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter">
                              <div class="flex items-center mr-4">
                                <img class="h-8 rounded-full" src="<%= item.image_account %>">
                                <p class="inter ml-2"><%= item.name_account %></p>
                              </div>
                            </td>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold"><%= item.pages_ids.length + item.groups_ids.length %> no total</td>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <% if (item.platform == "Facebook") { %>
                                <p style="color: #1877F2" class="inter font-semibold"><%= item.platform %></p>
                              <% } else { %>
                                <p style="color: #C21F70" class="inter font-semibold"><%= item.platform %></p>
                              <% } %>
                            </td>
                            <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 inter color-purple font-semibold">
                              <% if(item.program_post == false) { %>
                                <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                  <div class="flex-none rounded-full p-1 text-green-400 bg-green-opacity">
                                    <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                  </div>
                                  <div class="hidden text-green-400 sm:block">Publicado</div>
                                </div>
                              <% } else { %>
                                <div class="flex items-center justify-end gap-x-2 sm:justify-start">
                                  <div class="flex-none rounded-full p-1 text-blue-500 bg-blue-opacity">
                                    <div class="h-1.5 w-1.5 rounded-full bg-current"></div>
                                  </div>
                                  <div class="hidden text-blue-500 sm:block">Programado</div>
                                </div>
                              <% } %>
                            </td>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
                  </div>
                <% } else { %>
                  <div class="flex flex-col justify-center items-center px-4 sm:px-6 lg:pl-12 mt-2 col-span-3 mt-40">
                    <svg width="284" height="215" viewBox="0 0 284 215" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <g clip-path="url(#clip0_135_57)">
                      <g filter="url(#filter0_d_135_57)">
