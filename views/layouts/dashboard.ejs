                      </g>
                      <path d="M142.47 4.65039H16.1521C13.1022 4.65381 10.1782 5.86689 8.02155 8.02349C5.86495 10.1801 4.65186 13.1041 4.64844 16.154V164.477C4.65187 167.527 5.86495 170.451 8.02156 172.607C10.1782 174.764 13.1022 175.977 16.1521 175.98H142.47C145.52 175.977 148.444 174.764 150.6 172.607C152.757 170.451 153.97 167.527 153.973 164.477V16.154C153.97 13.1041 152.757 10.1801 150.6 8.02354C148.444 5.86694 145.52 4.65384 142.47 4.65039Z" fill="white"/>
                      <path d="M123.462 47.7788H59.5846C58.8711 47.7788 58.1869 47.4954 57.6824 46.9909C57.1779 46.4864 56.8945 45.8022 56.8945 45.0888C56.8945 44.3753 57.1779 43.6911 57.6824 43.1866C58.1869 42.6821 58.8711 42.3987 59.5846 42.3987H123.462C123.816 42.3987 124.166 42.4683 124.492 42.6035C124.818 42.7386 125.115 42.9368 125.365 43.1866C125.614 43.4364 125.813 43.7329 125.948 44.0593C126.083 44.3857 126.153 44.7355 126.153 45.0888C126.153 45.442 126.083 45.7918 125.948 46.1182C125.813 46.4446 125.614 46.7411 125.365 46.9909C125.115 47.2407 124.818 47.4389 124.492 47.574C124.166 47.7092 123.816 47.7788 123.462 47.7788Z" fill="#F2F2F2"/>
